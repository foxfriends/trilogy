proc inner!() {
  let r = yield 'in
  assert "resumed value is 'a" as r == 'a
  yield 'out

  assert "unreachable" as false
}

proc main!() {
  let x = with inner!() {
    when 'in then {
      if resume 'a
        then cancel 1
        else cancel 2
    }
    when 'out cancel false
    else yield
  }
  assert "final result should be 2 but it was ${x}" as x == 2
}
