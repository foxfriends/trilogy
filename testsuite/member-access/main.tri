proc main!() {
  assert [1, 2, 3].2 == 3
  assert [1, 2, 3].0 == 1
  assert with [1, 2, 3].4 { when 'mia cancel true else cancel false }

  assert "hello".4 == 'o'
  assert with "hello".5 { when 'mia cancel true else cancel false }

  assert {|'a' => 0, 'b' => 1, 'c' => 2|}.'a' == 0
  assert {|0 => 'a', 1 => 'b', 2 => 'c'|}.0 == 'a'
  assert with {|0 => 'a', 1 => 'b', 2 => 'c'|}.3 { when 'mia cancel true else cancel false }
}
