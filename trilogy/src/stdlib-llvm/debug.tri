module libc at "trilogy:c"

export dbg
proc dbg!(value) {
  if value == unit {
    libc::printf!("unit")
  } else if value == true {
    libc::printf!("true")
  } else if value == false {
    libc::printf!("false")
  } else if (typeof value) == 'string {
    libc::printf!(value)
  } else {
    libc::printf!("unsupported value")
  }
  return value
}
