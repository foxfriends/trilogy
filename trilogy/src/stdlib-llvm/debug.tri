module core at "trilogy:core"

export dbg
proc dbg!(value) {
  if value == unit {
    core::trilogy_print!("unit")
  } else if value == true {
    core::trilogy_print!("true")
  } else if value == false {
    core::trilogy_print!("false")
  } else if typeof value == 'string {
    core::trilogy_print!(value)
  } else if typeof value == 'atom {
    core::trilogy_print!(core::trilogy_lookup_atom!(value))
  } else {
    core::trilogy_print!("unsupported value")
  }
  return value
}
