module core at "trilogy:core"

export dbg
proc dbg!(value) {
  if value == unit {
    core::trilogy_printf!("unit")
  } else if value == true {
    core::trilogy_printf!("true")
  } else if value == false {
    core::trilogy_printf!("false")
  } else if (typeof value) == 'string {
    core::trilogy_printf!(value)
  } else {
    core::trilogy_printf!("unsupported value")
  }
  return value
}
