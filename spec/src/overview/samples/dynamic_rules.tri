proc main!() {
    let line = readline!() when 'EOF cancel end 1

    let allowed1 = is allow(line)
        given allow("cat")

    let allowed2 = is allow(line)
        given allow("cat")
        given allow("cactus")

    let allowed3 = is allow(line)
        given allow("cat")
        given allow("cactus")
        given allow("cash")

    println!("results: ${allowed1}; ${allowed2}; ${allowed3}")
}
